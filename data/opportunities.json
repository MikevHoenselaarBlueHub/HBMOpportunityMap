// project_root/css/style.css
/* Basic CSS styling */
body {
    font-family: sans-serif;
}

.municipality {
    stroke: black;
    stroke-width: 0.5px; /* Thinner lines by default */
    fill: lightgray;
}

.highlighted {
    stroke-width: 2px; /* Thicker lines when highlighted */
}

#map {
    width: 800px;
    height: 600px;
    border: 1px solid black;
}

// project_root/js/script.js
// Basic JavaScript for map interaction
function initMap() {
    console.log("Map initialized");
}

initMap();

